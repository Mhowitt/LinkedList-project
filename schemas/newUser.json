{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "firstName": {
      "$id": "/properties/firstName",
      "type": "string",
      "title": "The Firstname Schema ",
      "examples": ["Jane"],
      "minLength": 1,
      "maxLength": 55
    },
    "lastName": {
      "$id": "/properties/lastName",
      "type": "string",
      "title": "The Lastname Schema ",
      "examples": ["Doe"],
      "minLength": 1,
      "maxLength": 55
    },
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "title": "The Username Schema ",
      "examples": ["linkedlistUser"],
      "minLength": 1,
      "maxLength": 55
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "format": "email",
      "title": "The Email Schema ",
      "examples": ["jane.doe@example.com"]
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "examples": ["password123"],
      "minLength": 1,
      "maxLength": 55
    },
    "photo": {
      "$id": "/properties/photo",
      "type": "string",
      "title": "The Photo Schema ",
      "default":
        "https://worldarts2015.s3-us-west-2.amazonaws.com/images/default-profile-picture.jpg?cache=1473463677",
      "examples": ["https://wwww.picture.com"]
    }
  },
  "required": ["firstName", "lastName", "username", "email", "password"]
}
