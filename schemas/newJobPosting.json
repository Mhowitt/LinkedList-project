{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "default": "",
      "examples": ["Software Engineer"],
      "minLength": 1,
      "maxLength": 55
    },
    "companyHandle": {
      "$id": "/properties/companyHandle",
      "type": "string",
      "title": "The Companyhandle Schema ",
      "default": "",
      "examples": ["rithmschool"]
    },
    "salary": {
      "$id": "/properties/salary",
      "type": "integer",
      "title": "The Salary Schema ",
      "default": 0,
      "examples": [120000]
    },
    "equity": {
      "$id": "/properties/equity",
      "type": "integer",
      "title": "The Equity Schema ",
      "default": 0,
      "examples": [8]
    }
  },
  "required": ["title", "companyHandle", "salary", "equity"]
}
